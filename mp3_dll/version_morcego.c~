#include "aline_definitive_header_2_.h"
#include <windows.h>
int __stdcall
ControlName (char *data)
{
  strcpy (data, "BW Player DLL " AMARIC__VERSION
#ifdef _M_X64
	  " 64 bits"
#else
	  " 32 bits"
#endif
    );
  return 0;
}
