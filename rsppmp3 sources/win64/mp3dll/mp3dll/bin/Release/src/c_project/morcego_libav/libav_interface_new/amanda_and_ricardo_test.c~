int main()
{
printf("Oi");
int amanda_counter = 0;

	while(1)
	{
		
		printf("Valeu my love... %d %d \n", amanda_counter,amanda_counter % 4);//se usar amanda_counter %  4 a contagem vai de 0 ate 3

		amanda_counter++;
		Sleep(500);


	}
return 0;
}
