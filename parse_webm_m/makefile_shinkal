	cp --preserve=timestamps parse_espirita.dll 'C:\cygwin64_amandaricardo_koci_10.2.0\bin\mp3dll'

shinkal_libs:
	x86_64-w64-mingw32-g++ -Iinclude -I. -std=c++11 -c src/*.cc
	x86_64-w64-mingw32-ar rcs libwebm.a *.o

exec_shinkal:
